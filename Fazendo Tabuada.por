programa {
	funcao inicio() {
	inteiro contador,limite,resultado,numero
	
	contador=0
	
	escreva("Qual tabuada você quer fazer?")
	leia(numero)
	escreva("Deseja multiplicar até qual número?")
	leia(limite)

	faca{
	    resultado=numero*contador
	    escreva(numero+"X"+contador+"="+resultado+"\n")
	
	    contador++

	}enquanto(contador<=limite)
	

	
	}
}